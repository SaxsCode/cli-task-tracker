#!/usr/bin/php

<?php

// argc - argv

$command = $argv[1];

switch ($command) {
    case 'add':
        break;
    case 'update':
        break;
    case 'delete':
        break;
    case 'mark-in-progress':
        break;
    case 'mark-done':
        break;
    case 'list':
        break;
}

readline($argv[1]);

